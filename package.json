{
  "name": "pinterest-scraper-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3017 --turbopack",
    "build": "next build --turbopack",
    "start": "next start -p 3017",
    "start:dev": "npm run dev",
    "start:prod": "npm run build && next start -p 3017",
    "start:clean": "bash -lc 'PIDS=$(lsof -ti tcp:3017 || true); if [ -n \"$PIDS\" ]; then echo \"$PIDS\" | xargs -r kill -9 || true; fi; next dev -p 3017 --turbopack'",
    "lint": "eslint"
  },
  "dependencies": {
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "playwright": "^1.55.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
